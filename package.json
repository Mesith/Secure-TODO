  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/react-native": "^12.1.2",
    "eslint": "^8.45.0",
    "prettier": "3.0.0"
