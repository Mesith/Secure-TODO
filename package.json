    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.22.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.4"
